
// Testing: Calling a function out of scope

fdef product(i: int, j:int) : int {
     return i * j;
}

fdef product(i:float, j:float) : float {
     return i * j;
}

p0: int = product(2,3);

{

  p1 : int = product(-20,10);
  p2 : float = product(-20.0,10.0);

  if (p1 < p2 || p1 == p2) {
     b : bool =  p1 * p2 / (p1 * p2) >= 200;
  } else {
     /* do something else */
  }
}